<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AlunoController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@../img/home.png" />
         </image>
      </ImageView>
      <TextField fx:id="textFieldPesquisarAluno" prefHeight="25.0" prefWidth="409.0" promptText="Pesquisa" AnchorPane.rightAnchor="0.0" />
      <TableView fx:id="tableAluno" prefHeight="375.0" prefWidth="409.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <columns>
            <TableColumn fx:id="tableColumnAlunoId" prefWidth="50.0" text="ID" />
            <TableColumn fx:id="tableColumnAlunoNome" prefWidth="120.0" text="Nome" />
            <TableColumn fx:id="tableColumnAlunoPlano" prefWidth="50.0" text="Plano" />
            <TableColumn fx:id="tableColumnAlunoCpf" prefWidth="120.0" text="CPF" />
         </columns>
      </TableView>
      <Button layoutX="26.0" layoutY="179.0" mnemonicParsing="false" onAction="#handleButtonPlano" prefHeight="26.0" prefWidth="86.0" text="Planos" />
      <Button layoutX="26.0" layoutY="221.0" mnemonicParsing="false" onAction="#handleButtonCadastro" prefHeight="26.0" prefWidth="86.0" text="Cadastro" />
      <Button layoutX="26.0" layoutY="262.0" mnemonicParsing="false" onAction="#handleButtonAluno" prefHeight="26.0" prefWidth="86.0" text="Alunos" />
   </children>
</AnchorPane>
